/*
    Agrupar propiedades por califiaciones
*/

use('sample_airbnb')

db.listingsAndReviews.aggregate([

    {
        $bucketAuto: {
            groupBy: "$review_scores.review_scores_rating",
            buckets: 4,
            output: {
                count: { $sum: 1 }
            }
        }
    }


])





/*
    Para dar rangos 
    BUCKET: LOS RANGOS SE DAN MANUALMENTE
    BUCKET AUTO: LOS RANGOS LOS PONE EL SISTEMA

*/